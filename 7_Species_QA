                     dat4$LengthType=="PSCFL"]<-dat4$newLngtClass[dat4$estsciname=="Hydrolagus mirabilis"&
                                                                            dat4$LengthType=="PSCFL"]*1.28
dat4$LengthType[dat4$family=="Chimaeridae"&
                  dat4$LengthType=="PSCFL"]<-"TL"
