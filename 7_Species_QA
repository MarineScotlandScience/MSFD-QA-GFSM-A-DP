                   dat4$LengthType=="PAFL"]<-3.1*dat4$newLngtClass[dat4$estsciname=="Trachyrincus scabrus"&
                                                 dat4$LengthType=="PAFL"]
dat4$LengthType[dat4$estsciname=="Trachyrincus scabrus"&
                  dat4$LengthType=="PAFL"]<-"TL"
